.big_box{
    padding: 0 20rpx;
}
.item_coll{
    display: flex;
    justify-content: space-between;
    margin-bottom: 40rpx;
}
.item_coll image{
    width: 220rpx;
    height: 300rpx;
    margin-right: 10rpx;
}
.item_coll_right{
    width:474rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    font-size: 13px;
    color: #757575;
}
.item_biaoti{
    width:474rpx;
    height: 40rpx;
    color: #333;
    font-size: 15px;
    overflow: hidden;
}

.item_t text{
  float: right;
}
.item_t progress{
  width: 300rpx;
  margin-top: 10rpx;
}
.item_b{
    display: flex;
    justify-content: space-between;
}
.item_b .item_b_l{
    width: 330rpx;
    height: 80rpx;
    overflow: hidden;
}
.item_b .item_b_l .wz1{
  float: left;
  height: 80rpx;
}
.item_b .item_b_r{
    text-align: right;
}
.item_btn{
    display: flex;
    justify-content: space-around;
}
.item_btn button{
    height:56rpx;
    margin:0;
    line-height:56rpx;
    text-align:center;
    background:#e9e9e9;
    border-radius:10rpx;
    border:none;
    font-size:12px;
    color:#333;
}